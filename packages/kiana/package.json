{
  "name": "kiana",
  "version": "0.1.0",
  "description": "Kiana - Minimal headless coding agent",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "kiana": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ai": "beta",
    "@ai-sdk/anthropic": "beta",
    "@ai-sdk/openai": "beta",
    "@ai-sdk/openai-compatible": "beta",
    "@ai-sdk/google": "beta",
    "zod": "latest",
    "jsonc-parser": "latest"
  },
  "devDependencies": {
    "typescript": "latest",
    "@types/node": "latest"
  },
  "engines": {
    "node": ">=20"
  },
  "license": "MIT"
}
